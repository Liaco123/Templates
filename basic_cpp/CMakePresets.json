{
  "version": 6,
  "cmakeMinimumRequired": {
    "major": 3,
    "minor": 23,
    "patch": 0
  },
  "configurePresets": [
    {
      "name": "dbg",
      "displayName": "Debug (Ninja + Conan2)",
      "generator": "Ninja",
      "binaryDir": "${sourceDir}/build/Debug",
      "cacheVariables": {
        "CMAKE_BUILD_TYPE": "Debug",
        "CMAKE_TOOLCHAIN_FILE": "${sourceDir}/.deps/Debug/conan_toolchain.cmake"
      }
    },
    {
      "name": "rel",
      "displayName": "Release (Ninja + Conan2)",
      "generator": "Ninja",
      "binaryDir": "${sourceDir}/build/Release",
      "cacheVariables": {
        "CMAKE_BUILD_TYPE": "Release",
        "CMAKE_TOOLCHAIN_FILE": "${sourceDir}/.deps/Release/conan_toolchain.cmake"
      }
    }
  ],
  "buildPresets": [
    {
      "name": "dbg",
      "configurePreset": "dbg"
    },
    {
      "name": "rel",
      "configurePreset": "rel"
    }
  ],
  "testPresets": [
    {
      "name": "dbg",
      "configurePreset": "dbg"
    },
    {
      "name": "rel",
      "configurePreset": "rel"
    }
  ]
}